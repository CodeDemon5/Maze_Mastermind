$('#table-active').click(function () {
    $('#table-active .btn-down').show(); 
});

$('#table-active .btn-down').click(function () {
    $('.table-listing').toggle(); 
    if (isOpen) {
         isOpen = false;
         $('#bi-open').removeClass('bi-chevron-down');  
         $('#bi-open').addClass('bi-chevron-up'); 
    }
    else {
        isOpen = true;
        $('#bi-open').addClass('bi-chevron-down');  
        $('#bi-open').removeClass('bi-chevron-up');   
    }
});

$(window).scroll(function(){
    if ($(this).scrollTop() > 300) {
       $('#table-active').addClass('table-active');
        $('.table-listing').removeClass('table-block');
        $('#btn-table').removeClass('d-none');
    } else {
       $('#table-active').removeClass('table-active'); 
        $('#btn-table').addClass('d-none');
        $('.table-listing').addClass('table-block');        
    }
});

